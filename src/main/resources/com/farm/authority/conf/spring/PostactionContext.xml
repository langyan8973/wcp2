<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
  <!-- 岗位权限Dao实现 -->
  <bean id="farm_authority_PostAction_DaoId"
    class="com.farm.authority.dao.impl.PostactionDaoImpl">
    <property name="sessionFatory">
      <ref bean="sessionFactory" />
    </property>
    <property name="sqlTools">
      <bean  class="com.farm.core.sql.utils.HibernateSQLTools">
        <constructor-arg>
           <ref bean="sessionFactory" />
        </constructor-arg>
        <constructor-arg>
          <bean class="com.farm.authority.domain.Postaction"></bean>
        </constructor-arg>
      </bean>
    </property>
  </bean>
  <!-- Server配置文件注入   -->
  <!-- Server实现类变量  -->
</beans>