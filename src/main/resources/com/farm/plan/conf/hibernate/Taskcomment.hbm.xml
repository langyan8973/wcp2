<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
   功能:任务留言,作者:FarmCode代码工程,日期:20141204174206
-->
<hibernate-mapping>
    <class name="com.farm.plan.domain.Taskcomment" table="farm_plan_taskcomment">
        <id name="id" 
            type="java.lang.String">
            <column name="ID" length="32" />
            <generator class="uuid.hex" />
        </id>
        <property name="handleid" 
            type="java.lang.String">
            <column name="HANDLEID" length="32" not-null="true"  />
        </property>
        <property name="pcontent" 
            type="java.lang.String">
            <column name="PCONTENT" length="2048" not-null="true"  />
        </property>
        <property name="cuser" 
            type="java.lang.String">
            <column name="CUSER" length="32" not-null="true"  />
        </property>
        <property name="cusername" 
            type="java.lang.String">
            <column name="CUSERNAME" length="64" not-null="true"  />
        </property>
        <property name="ctime" 
            type="java.lang.String">
            <column name="CTIME" length="16" not-null="true"  />
        </property>
    </class>
</hibernate-mapping>