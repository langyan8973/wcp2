<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
   Farm代码工程
-->
<hibernate-mapping>
    <class name="com.farm.portal.domain.Portaltree" table="farm_portal_tree">
        <id name="id" 
            type="java.lang.String">
            <column name="ID" length="32" />
            <generator class="uuid.hex" />
        </id>
        <property name="componentdefid" 
            type="java.lang.String">
            <column name="COMPONENTDEFID" length="32"   />
        </property>
        <property name="portaldefid" 
            type="java.lang.String">
            <column name="PORTALDEFID" length="32"   />
        </property>
        <property name="cusername" 
            type="java.lang.String">
            <column name="CUSERNAME" length="64" not-null="true"  />
        </property>
        <property name="cuser" 
            type="java.lang.String">
            <column name="CUSER" length="32" not-null="true"  />
        </property>
        <property name="eusername" 
            type="java.lang.String">
            <column name="EUSERNAME" length="64" not-null="true"  />
        </property>
        <property name="euser" 
            type="java.lang.String">
            <column name="EUSER" length="32" not-null="true"  />
        </property>
        <property name="pstate" 
            type="java.lang.String">
            <column name="PSTATE" length="2" not-null="true"  />
        </property>
        <property name="pcontent" 
            type="java.lang.String">
            <column name="PCONTENT" length="128"   />
        </property>
        <property name="sort" 
            type="java.lang.Integer">
            <column name="SORT"  not-null="true"  />
        </property>
        <property name="type" 
            type="java.lang.String">
            <column name="TYPE" length="2" not-null="true"  />
        </property>
        <property name="treecode" 
            type="java.lang.String">
            <column name="TREECODE" length="256" not-null="true"  />
        </property>
        <property name="parentid" 
            type="java.lang.String">
            <column name="PARENTID" length="32" not-null="true"  />
        </property>
        <property name="name" 
            type="java.lang.String">
            <column name="NAME" length="32" not-null="true"  />
        </property>
        <property name="etime" 
            type="java.lang.String">
            <column name="ETIME" length="16" not-null="true"  />
        </property>
        <property name="ctime" 
            type="java.lang.String">
            <column name="CTIME" length="16" not-null="true"  />
        </property>
    </class>
</hibernate-mapping>